# AWS Serverless Data Lake - Frontend Environment Configuration
# Copy this file to .env.local and fill in your values

# API Gateway endpoint URL (from Terraform outputs)
VITE_API_BASE_URL=https://your-api-id.execute-api.eu-west-1.amazonaws.com/dev

# Alternative: Full query endpoint
VITE_API_ENDPOINT=https://your-api-id.execute-api.eu-west-1.amazonaws.com/dev/query

# Optional: AWS Region (for AWS SDK if needed)
VITE_AWS_REGION=eu-west-1

# Optional: Enable debug mode
VITE_DEBUG=false

